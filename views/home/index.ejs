  
           <% _.each(posts, function (post) { %>

           
          <div class="post">
            <div class="post-title">
              <a href="#" class="author"><img src="/images/avatar.jpg" alt=""></a>
              <h2 class="entry-title">
               <%= post.title %>
              </h2>
              <div class="post-meta">
                <ul>
                  <li><%= fmtDate(post.date,"MMM Do") %></li>
                  <li class="divider">/</li>
                  <li>Demo</li>
                  <li class="divider">/</li>
                  <li>0 Comments</li>

                </ul>
              </div>
            </div>
            <div class="entry-image">
              <img src="images/lua.jpg" alt="">
            </div>
            <div class="entry-container">
              <div class="entry-content">
                <p><%- post.text %></p>

                <footer class="entry-meta clearfix">
                  <ul class="tags">
                    <li>Welcome</li>
                  </ul>
                  <div class="share-container">
                    <h4>Share Story</h4>
                    <ul class="share-story">

                    </ul>
                  </div>
                </footer>
              </div>
            </div>
          </div>
          <!-- end post -->
          <% }) %>

        